{
  "name": "7dtd-map",
  "version": "1.0.0",
  "description": "Map renderer for 7 Days to Die",
  "private": true,
  "scripts": {
    "build": "npm run resources && npm run js",
    "resources": "node ./tools/generate-block-index.js && node ./tools/generate-prefab-html.js",
    "js": "webpack --mode production",
    "dev-server": "webpack serve --mode development --content-base docs",
    "lint": "npm run jslint && npm run htmllint && npm run csslint",
    "jslint": "eslint tools/**/*.js --fix",
    "htmllint": "htmlhint docs/**/*.html",
    "csslint": "stylelint docs/**/*.css --fix"
  },
  "author": "Keiichiro UI",
  "license": "MIT",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/bunyan-format": "^0.2.3",
    "@types/pngjs": "^6.0.0",
    "@types/xml2js": "^0.4.8",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "csv-parse": "^4.16.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "glob": "^7.1.7",
    "glob-promise": "^4.2.0",
    "htmlhint": "^0.15.1",
    "pako": "^2.0.3",
    "pngjs": "^6.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "xml2js": "^0.4.23"
  },
  "dependencies": {}
}
