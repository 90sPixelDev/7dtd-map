{
  "name": "7dtd-map",
  "version": "1.0.0",
  "description": "Map renderer for 7 Days to Die",
  "private": true,
  "scripts": {
    "build": "npm run json && npm run js",
    "json": "node ./tools/generate-block-index.js && node ./tools/generate-prefab-html.js",
    "js": "webpack --mode production",
    "dev-server": "webpack-dev-server --mode development --content-base docs",
    "lint": "npm run jslint && npm run htmllint && npm run csslint",
    "jslint": "eslint {js,tools}/**/*.js --fix",
    "htmllint": "htmlhint docs/**/*.html",
    "csslint": "stylelint docs/**/*.css --fix"
  },
  "author": "Keiichiro UI",
  "license": "MIT",
  "devDependencies": {
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "commander": "^2.19.0",
    "csv-parse": "^4.6.3",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "glob": "^7.1.3",
    "glob-promise": "^3.4.0",
    "htmlhint": "^0.10.1",
    "jimp": "^0.8.4",
    "lodash": "^4.17.15",
    "pako": "^1.0.7",
    "pngjs": "^3.4.0",
    "stream-to-blob": "^2.0.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1",
    "xml2js": "^0.4.19"
  },
  "dependencies": {}
}
