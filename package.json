{
  "name": "7dtd-map",
  "version": "1.0.0",
  "description": "Map renderer for 7 Days to Die",
  "private": true,
  "scripts": {
    "dev-server": "webpack serve --mode development --content-base docs",
    "build": "run-p build:*",
    "build:ts": "webpack --mode production",
    "build:json": "ts-node --files ./tools/generate-block-index.ts",
    "build:prefab-html": "ts-node --files ./tools/generate-prefab-html.ts",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint --ext .ts --fix .",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --ext .ts .",
    "lint:tsc:src": "tsc -p src",
    "lint:tsc:tools": "tsc -p tools"
  },
  "author": "Keiichiro UI",
  "license": "MIT",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/bunyan-format": "^0.2.3",
    "@types/css-font-loading-module": "0.0.4",
    "@types/node": "^15.12.4",
    "@types/pako": "^1.0.1",
    "@types/pngjs": "^6.0.0",
    "@types/webpack": "^5.28.0",
    "@types/xml2js": "^0.4.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "csv-parse": "^4.16.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "glob": "^7.1.7",
    "glob-promise": "^4.2.0",
    "idb": "^6.1.2",
    "npm-run-all": "^4.1.5",
    "pngjs": "^6.0.0",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "xml2js": "^0.4.23"
  },
  "dependencies": {}
}
